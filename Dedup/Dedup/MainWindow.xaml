<Window x:Class="Dedup.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dedup"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" WindowStartupLocation="CenterScreen">
    <Grid>
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="246" VerticalAlignment="Top" Width="517">
            <TabItem Header="Backup" Name="Backup">
                <Grid Background="#FFE5E5E5">
                    <TreeView x:Name="treeView" HorizontalAlignment="Left" Height="177" VerticalAlignment="Top" Width="511" />
                    <Button x:Name="btnSelect" Content="Select" HorizontalAlignment="Left" Margin="10,188,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.066,0.283" Click="btnSelect_Click"/>
                </Grid>
            </TabItem>
            <TabItem Header="Restore">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
        

    </Grid>
</Window>
